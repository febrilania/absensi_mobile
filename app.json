{
  "expo": {
    "name": "SIMBARU",
    "slug": "simbaru",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/simbaru.png",
    "scheme": "simbaru",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/simbaru.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chuenk.simbaru",
      "buildNumber": "8"
    },
    "android": {
      "versionCode": 8,
      "package": "com.chuenk.simbaru",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/simbaru.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/simbaru.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Aplikasi ini membutuhkan akses kamera untuk memindai QR Code",
          "microphonePermission": "Diperlukan jika ingin merekam video"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/simbaru.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#ffffff"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0b851e8f-5727-4e8e-be5b-e953619ab847"
      }
    }
  }
}
